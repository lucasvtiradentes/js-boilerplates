{
  "name": "aws-lambda-puppeteer",
  "version": "0.0.1",
  "description": "description",
  "license": "MIT",
  "author": {
    "name": "your name",
    "email": "youremail@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/repository.git"
  },
  "scripts": {
    "prepare": "husky install",
    "precommit": "npm run script:issues",
    "commit": "cz",
    "function:prod": "serverless invoke --function puppeteerFunction",
    "function:dev": "serverless invoke local --function puppeteerFunction",
    "dev": "serverless offline start",
    "build": "serverless package",
    "build:check": "tsc --noEmit",
    "pub": "serverless deploy",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged --concurrent false --quiet",
    "script:issues": "ts-node ./scripts/github-issues.ts",
    "script:gitmoji": "ts-node ./scripts/gitmoji-setup.ts"
  },
  "dependencies": {
    "chrome-aws-lambda": "^10.1.0",
    "puppeteer-core": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/node": "^18.14.2",
    "@types/puppeteer-core": "^7.0.4",

    "commitizen": "^4.3.0",
    "commitizen-emoji": "^1.0.5",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",

    "lint-staged": "^13.1.0",
    "prettier": "^2.8.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint-plugin-prettier": "^4.2.1",

    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",

    "typescript": "^4.9.5",
    "husky": "^8.0.3",
    "ts-node": "^10.9.1"
  }
}
